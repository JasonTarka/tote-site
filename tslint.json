{
  "rules": {
    "align": [
      true,
      "arguments",
      "statements"
    ],
    "ban": [
      true,
      ["console","log"],
      ["console","info"],
      ["describe","only"],
      ["it","only"]
    ],
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": true,
    "indent": "tabs",
    "no-any": false,
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-debugger": true,
    "no-duplicate-key": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-trailing-whitespace": true,
    "no-switch-case-fall-through": true,
    "no-unused-expression": true,
    "no-unused-variable": [
      true,
      "check-parameters"
    ],
    "no-use-before-declare": true,
    "one-line": [
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": "single",
    "semicolon": true,
    "trailing-comma": {
      "multiline": true,
      "singleline": true
    },
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "use-strict": [
      true,
      "check-module"
    ],
    "whitespace": [
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-typecast"
    ]
  }
}